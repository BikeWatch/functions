{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "telemetric",
      "collectionName": "{uuid}",
      "createIfNotExists": true,
      "partitionKey": "id",
      "connectionStringSetting": "COSMOS_CONNECTION_URI"
    }
  ]
}
